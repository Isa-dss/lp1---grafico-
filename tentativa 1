DADOS

import java.util.Scanner;
public class GraficoDdos {

	public int Hour;
	public int Days;
	public int Mounth;
	
	public GraficoDdos (int Hour, int Days,int Mounth) {
		System.out.printf("\nInsira a hora:");
		this.Hour = Hour;
		
		System.out.printf("\nInsira os dias:");
		this.Days = Days;
		
		System.out.printf("\nInsira os meses:");
		this.Mounth = Mounth;
		
	}

	public int getHour() {
		return this.Hour;
		}
	public int getDays(){
		return this.Days;
	}
	
	public int getMounth() {
		return this.Mounth;
	}
	
		
	}
